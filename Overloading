using System;

namespace Overloading
{
    class Overloading
    {
        public void Fun()
        {
            System.Console.WriteLine("Fun function with no parameter");
        }
        public void Fun(int i)
        {
            System.Console.WriteLine("Fun function with integer type parameter");
        }
        public void Fun(string s)
        {
            System.Console.WriteLine("Fun function with string type sparameter");
        }
        public void Fun(int i,string s)
        {
            System.Console.WriteLine("Fun function with integer and string type parameter");
        }
        static void Main(string[] args)
        {
            Overloading o=new Overloading();
            o.Fun();
            o.Fun(5);
            o.Fun("s");
            o.Fun(5,"s");
        }
    }
}

output:
Fun function with no parameter
Fun function with integer type parameter
Fun function with string type sparameter
Fun function with integer and string type parameter
